<%= simple_form_for(@restaurant) do |f| %>
  <%= f.input :name %>
  <%= f.input :category, collection: ["chinese", "italian", "japanese", "french", "belgian"] %>
  <%= f.input :address %>
  <%= f.input :phone_number %>
  <%= f.submit "Create the restaurant", class: "btn btn-primary" %>
<% end %>
<%= link_to 'Back to the List', restaurants_path %>
